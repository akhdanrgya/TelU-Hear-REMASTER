<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<StackPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.tubes.teluhear.PremiumController">
   <children>
      <VBox alignment="CENTER" prefHeight="200.0" prefWidth="100.0" spacing="20.0" style="-fx-background-color: #dce6f5;">
         <children>
            <Label fx:id="premiumLabel1" alignment="CENTER" prefHeight="26.0" prefWidth="193.0" style="-fx-background-radius: 20;" text="Anda bukan user premium">
               <opaqueInsets>
                  <Insets />
               </opaqueInsets>
               <font>
                  <Font name="System Bold" size="14.0" />
               </font></Label>
            <Label fx:id="premiumLabel2" alignment="CENTER" prefHeight="28.0" prefWidth="195.0" style="-fx-background-radius: 20;" text="Dapatkan premium sekarang">
               <font>
                  <Font name="System Bold" size="14.0" />
               </font></Label>
            <Button fx:id="premiumButtonAction" alignment="CENTER" mnemonicParsing="false" onAction="#premiumButton" style="-fx-background-color: a3b9d9; -fx-background-radius: 20;" text="premium">
               <font>
                  <Font name="System Bold" size="14.0" />
               </font></Button>
         </children>
      </VBox>
   </children>
</StackPane>
